    "new_panet = \"\"\n",
    "planetas = []\n",
    "\n",
    "while new_panet.lower() != 'hecho':\n",
    "    if new_panet:\n",
    "        planetas.append(new_panet)\n",
    "    new_panet = input('Ingresa un nuevo planeta')\n",
    "\n",
    "\n",
    "for planeta in planetas:\n",
    "    print(planeta)"
